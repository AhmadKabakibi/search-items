{
  "name": "search-items-ts",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "author": "Ahmad Kabakibi<ahmadkbakibi@gmail.com>",
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn format && yarn test && yarn validate",
      "pre-push": "yarn test:coverage"
    }
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "validate": "yarn tsc --noEmit",
    "lint": "yarn lint:ts && yarn lint:css",
    "lint:ts": "tslint --fix -p \"tsconfig.json\" -t stylish \"src/**/*.{ts,tsx}\"",
    "lint:css": "stylelint \"./src/**/*.{ts,tsx}\"",
    "format": "prettier --list-different \"src/**/*.{js,jsx}\" \"src/**/*.{ts,tsx}\"",
    "test": "node scripts/test.js --colors",
    "test:watch": "node scripts/test.js --watch",
    "test:coverage": "node scripts/test.js --coverage"
  },
  "eslintConfig": {
    "env": {
      "jest": true,
      "browser": true,
      "node": true
    },
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 10",
    "not op_mini all"
  ],
  "dependencies": {
    "@types/jest": "24.0.0",
    "@types/node": "10.12.24",
    "@types/react": "16.8.2",
    "@types/react-dom": "16.8.0",
    "@types/react-helmet": "5.0.8",
    "@types/react-router": "4.4.3",
    "@types/react-router-dom": "4.3.1",
    "@types/styled-components": "4.1.10",
    "check-dependencies": "^1.1.0",
    "react": "16.8.1",
    "react-app-polyfill": "0.2.1",
    "react-dom": "16.8.1",
    "react-helmet": "5.2.0",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.1.5",
    "styled-components": "4.1.3",
    "styled-normalize": "8.0.6",
    "typescript": "3.3.3"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@svgr/webpack": "2.4.1",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "0.3.1",
    "babel-preset-react-app": "7.0.1",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "2.4.1",
    "css-loader": "1.0.0",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.9.1",
    "enzyme-to-json": "3.3.5",
    "eslint": "5.6.0",
    "eslint-config-react-app": "3.0.6",
    "eslint-loader": "2.1.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "7.0.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "husky": "1.3.1",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "jest-styled-components": "6.3.1",
    "mini-css-extract-plugin": "0.4.3",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.1.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.3.1",
    "postcss-safe-parser": "4.0.1",
    "prettier": "1.16.4",
    "react-dev-utils": "7.0.1",
    "resolve": "1.8.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "stylelint": "9.10.1",
    "stylelint-config-recommended": "2.1.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-order": "2.0.0",
    "stylelint-processor-styled-components": "1.5.2",
    "terser-webpack-plugin": "1.1.0",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-config-standard": "8.0.1",
    "tslint-plugin-prettier": "2.0.1",
    "url-loader": "1.1.1",
    "webpack": "4.19.1",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "functions": 80,
        "lines": 80
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      ".+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      ".+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      ".+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "react-native$": "react-native-web",
      ".+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/testSetup.js"
  }
}
